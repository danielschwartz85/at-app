{
  "name": "problem-solver",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "jss-rtl": "^0.2.3",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-jss": "^8.6.1",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.45"
  },
  "scripts": {
    "start": "REACT_APP_SITE_PATH='at/..' react-scripts start",
    "build": "REACT_APP_SITE_PATH=action-therapy react-scripts build",
    "build:pub":"npm run build && npm run build:fix && npm run build:cp && npm run build:push",
    "build:fix":"sed -i 's/\\/static\\/css\\//\\/action-therapy\\/static\\/css\\//g' build/index.html && sed -i 's/\\/static\\/js\\//\\/action-therapy\\/static\\/js\\//g' build/index.html",
    "build:push": "MSG=$(git log -1 --pretty=%s) && cd ../action-therapy-site && git add . && (git diff-index --quiet HEAD || git commit -m '`$MSG`') && git push origin master",
    "build:cp":"cp -rf build/* ../action-therapy-site",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
